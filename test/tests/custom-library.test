#!/usr/bin/env bash

PATH=${PWD}/fixtures/custom-library:${PATH}
output=$(mktemp)
trap "rm -r -- '${output}'" EXIT

test-init-custom-library() {
  cli command &> "${output}"
  diff "${output}" - <<EOF
loading cli/init
EOF
}

test-load-custom-library() {
  cli command --load-test-lib &> "${output}"
  diff "${output}" - <<EOF
loading cli/init
loading cli/test
EOF
}

. ./lib/run-tests
