#!/usr/bin/env bash
# shellcheck disable=SC2034


OPTIONS=(
  boolean boolean
  single  single
  multi   multi
  first   enum:ignored
  second  enum:ignored
)

OPTION_DESCRIPTIONS=(
  boolean 'boolean option'
  single  'single option'
  multi   'multi option'
