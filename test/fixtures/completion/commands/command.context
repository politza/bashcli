#!/usr/bin/env bash
# shellcheck disable=SC2034


OPTIONS=(
  boolean    boolean
  single     single
  multi      multi
  multi-raw  multi-raw
  first      enum:ignored
  second     enum:ignored
)

OPTION_DESCRIPTIONS=(
  boolean   'boolean option'
  single    'single option'
  multi     'multi option'
  multi-raw 'multi-raw option'
  first     'first enum option'
  second    'second enum option'
)

OPTION_ALIASES=(
  b boolean
  a boolean
  c boolean
  s single
  m multi
  M multi-raw
  f first
)

OPTION_VALUE_TYPES=(
  boolean    none
  single     single
  multi      multi
  multi-raw  multi-raw
  %parameter parameter
)
