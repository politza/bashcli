#!/usr/bin/env bash

# shellcheck disable=SC2034

OPTIONS=(
  boolean boolean
  single  single
  multi   multi
  first   enum:ignored
  second  enum:ignored
)

OPTION_DESCRIPTIONS=(
  boolean 'boolean option'
  single  'single option'
  multi   'multi option'
  first   'first enum option'
  second  'second enum option'
)

OPTION_ALIASES=(
  b boolean
  s single
  m multi
  f first
  s second
)

OPTION_COMPLETIONS=(
  boolean none
  single     single-0,single-1
  multi      multi
  %parameter parameter
)
