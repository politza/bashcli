#!/usr/bin/env bash

# shellcheck disable=SC2034

DESCRIPTION="
Unsets a given property.
"

OPTIONS=(
  profile single
)

OPTION_DESCRIPTIONS=(
  profile "The profile for which to unset the property."
)

OPTION_ALIASES=(
  P profile
)

OPTION_VALUE_TYPES=(
  %parameter-0 config-property
  profile config-profile
)
