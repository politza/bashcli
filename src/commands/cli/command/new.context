#!/usr/bin/env bash
# shellcheck disable=SC2034

DESCRIPTION="
Creates new commands.
"

OPTIONS=(
  force     flag
  directory single
)

OPTION_DESCRIPTIONS=(
  force     "Overwrite already existing files."
  directory "The directory in which to create the command files."
)

OPTION_ALIASES=(
  f force
  d directory
)

OPTION_VALUE_TYPES=(
  directory directory
  %parameter command-paths
)
