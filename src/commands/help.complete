#!/usr/bin/env bash

# shellcheck disable=SC2034
bashcli-complete-help-for-command() {
  local commandIndex=${BASHCLI_COMPLETION_PARAMETER_INDEX}
  local commandMain commandList subcommand completion
  local _BASHCLI_COMMAND_STACK=(.)

  # shellcheck disable=SC2154
  if bashcli-options-parse '' commandList -- "${words[@]:1}"; then
    commandList=(. "${commandList[@]:1:${commandIndex}}")
    bashcli-complete-subcommand "${commandList[@]}"
  fi
}
